def main():

    def arbeit(возраст):
        if 60 <= возраст < 65:
            return 'Вы на пенсии'
        elif 21 <= возраст <= 59:
            return 'Вы работаете'
        elif 18 <= возраст <= 21:
            return 'Вы учитесь в ВУЗе'
        elif 7 <= возраст < 17:
            return 'Вы ходите в школу'
        else:
            return 'Вы ходите в ДС'

    u_возраст = arbeit(int(input('Сколько вам лет: ')))
    print(u_возраст)

if __name__ == "__main__":
    main()
